{
  "connection": "default",
  "info": {
    "name": "user",
    "description": ""
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "goods": {
      "collection": "goods",
      "via": "owner"
    },
    "businessprofile": {
      "model": "businessprofile",
      "via": "user"
    },
    "following_follows": {
      "collection": "follow",
      "via": "follower"
    },
    "follows": {
      "collection": "follow",
      "via": "followee"
    },
    "reviews": {
      "collection": "review",
      "via": "owner"
    },
    "questions": {
      "collection": "question",
      "via": "owner"
    }
  },
  "collectionName": "users-permissions_user"
}